<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="asywalul.finddifferen.android" >

    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/icon_"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name=".HomeActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LostActivity"/>
        <activity android:name=".NextGameActivity"/>
        <activity android:name=".MainActivity"/>
        <activity android:name=".PlayActivity"/>
        <activity android:name=".StagePickActivity"/>
        <activity android:name=".SummaryActivity"/>

    </application>

</manifest>